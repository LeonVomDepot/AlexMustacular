{
  "Format": "2.0.0",
  "ConfigSchema": {
    "EnabledSetting": {
      "AllowValues": "true, false",
      "Default": true
    },
    "SeasonalOutfitsSetting": {
      "AllowValues": "disabled, enabled, auto",
      "Default": "auto",
      "Section": "appearance"
    },
    "ExtraFestivalOutfitSetting": {
      "AllowValues": "true, false",
      "Default": true,
      "Section": "appearance"
    }
  },
  "DynamicTokens": [
    {
      "Name": "OutfitType",
      "Value": "fall"
    },
    {
      "Name": "OutfitType",
      "Value": "{{season}}",
      "When": {
        "SeasonalOutfitsSetting": "enabled"
      }
    },
    {
      "Name": "OutfitType",
      "Value": "{{season}}",
      "When": {
        "SeasonalOutfitsSetting": "auto",
        "HasMod": "Poltergeister.SeasonalCuteCharacters"
      }
    }
  ],
  "Changes": [
    {
      "Action": "Include",
      "FromFile": "data/body.json",
      "When": {
        "EnabledSetting": true
      }
    },
    {
      "Action": "Include",
      "FromFile": "data/outfit.json",
      "When": {
        "EnabledSetting": true
      }
    },
    {
      "Action": "Include",
      "FromFile": "data/outfit_festival.json",
      "When": {
        "EnabledSetting": true,
        "ExtraFestivalOutfitSetting": true
      }
    }
  ]
}
